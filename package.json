{
  "name": "Electron-sample-app",
  "version": "1.0.0",
  "description": "Basic Electron sample app",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Electron-sample-app",
    "build-linux32": "electron-packager . filesync --platform=linux  --arch=ia32",
    "build-linux64": "electron-packager . filesync --platform=linux  --arch=x64",
    "build-mac": "electron-packager . filesync --platform=darwin  --arch=x64",
    "build-win32": "electron-packager . filesync --platform=win32  --arch=ia32",
    "build-win64": "electron-packager . filesync --platform=win32  --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linkRachit/Electron-sample-app.git"
  },
  "author": "Rachit Goel<rachit2goel@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/linkRachit/Electron-sample-app/issues"
  },
  "homepage": "https://github.com/linkRachit/Electron-sample-app#readme",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-packager": "^8.5.2"
  }
}
